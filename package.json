{
  "type": "module",
  "version": "3.2.2",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "lint": "eslint . --cache",
    "release": "bumpp",
    "publish:ci": "pnpm -r publish --access public --no-git-checks",
    "test": "FORCE_COLOR=3 && vitest",
    "typecheck": "vue-tsc --noEmit",
