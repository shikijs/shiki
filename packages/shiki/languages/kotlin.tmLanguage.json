            },
            {
              "match": "\\b(0(b|B)[0-1_]*)([LuU]|[uU]L)?\\b",
              "name": "constant.numeric.binary.kotlin"
            },
            {
              "match": "\\b([0-9][0-9_]*\\.[0-9][0-9_]*[fFL]?)\\b",
              "name": "constant.numeric.float.kotlin"
