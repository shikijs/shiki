        {
          "include": "#numeric"
        },
        {
          "include": "#string"
        },
        {
          "include": "#null"
        }
      ],
      "repository": {
        "boolean": {
          "patterns": [
            {
              "match": "\\b(true|false)\\b",
              "name": "constant.language.boolean.kotlin"
            }
          ]
        },
        "numeric": {
          "patterns": [
            {
              "match": "\\b(0(x|X)[0-9A-Fa-f_]*)([LuU]|[uU]L)?\\b",
