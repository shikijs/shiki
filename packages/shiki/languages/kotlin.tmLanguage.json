        {
          "begin": "\\[",
          "beginCaptures": {
            "0": {
              "name": "punctuation.section.brackets.begin.kotlin"
            }
          },
          "end": "\\]",
          "endCaptures": {
            "0": {
              "name": "punctuation.section.brackets.end.kotlin"
            }
          },
          "name": "meta.brackets.kotlin",
          "patterns": [
            {
              "include": "#code"
            }
          ]
