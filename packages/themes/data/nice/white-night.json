  "tokenColors": [
    {
      "scope": [
        "comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#2e2e37"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#a8a8b1"
      }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "storage.type.string.python"
      ],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "scope": [
        "beginning.punctuation",
        "entity.name.section.group-title",
        "entity.name.tag",
