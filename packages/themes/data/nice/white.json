  "tokenColors": [
    {
      "scope": [
        "comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#bec9d3"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#6b7a88"
      }
    },
    {
